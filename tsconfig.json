{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es2019",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "lib",
        "strict": true,
        "resolveJsonModule": true,
        "declaration": true,
        "baseUrl": ".",
        "paths": {
            "gdocs-databse": [
                "node_modules/gdocs-databse/src"
            ],
            "gdocs-databse/*": [
                "node_modules/gdocs-databse/src/*"
            ]
        }
    },
    "lib": [
        "es2019"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts",
        "example",
        "lib",
        ".history"
    ]
}